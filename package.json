    "playwright": "1.31.1",
    "postcss": "8.4.21",
    "qs": "6.11.0",
    "react": "17.0.2",
    "requirejs": "2.3.6",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-visualizer": "5.9.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "url-search-params-polyfill": "8.1.1",
    "vite": "4.1.4",
    "vite-plugin-environment": "^1.1.3",
    "vue-tsc": "1.1.7",
    "vue3-gettext": "^2.3.3",
    "wait-for-expect": "3.0.2"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@7.18.1",
  "volta": {
    "node": "16.19.0"
  },
  "pnpm": {
    "packageExtensions": {
      "@adobe/leonardo-contrast-colors": {
        "dependencies": {
          "d3": "^5.12.0",
          "d3-cam02": "^0.1.5",
          "d3-hsluv": "^0.1.2",
          "d3-hsv": "^0.1.0"
        }
      },
      "jest-serializer-vue-tjw": {
        "dependencies": {
          "@vue/compiler-sfc": "2.7.14"
        }
      }
    },
    "patchedDependencies": {
      "@adobe/leonardo-contrast-colors@1.0.0-alpha.16": "patches/@adobe__leonardo-contrast-colors@1.0.0-alpha.16.patch",
      "jest-serializer-vue-tjw@3.19.0": "patches/jest-serializer-vue-tjw@3.19.0.patch",
      "keycode@2.2.1": "patches/keycode@2.2.1.patch",
      "vue-inline-svg@3.1.0": "patches/vue-inline-svg@3.1.0.patch"
