    "browserslist-useragent-regexp": "^3.0.2",
    "commander": "8.3.0",
    "core-js": "3.28.0",
    "cucumber-html-reporter": "5.5.0",
    "depcheck": "1.4.3",
    "ejs": "3.1.8",
    "eslint": "8.34.0",
    "flush-promises": "1.0.2",
    "git-repo-info": "2.1.1",
    "jest": "29.4.3",
    "jest-axe": "5.0.1",
    "jest-environment-jsdom": "29.4.3",
    "jest-fetch-mock": "3.0.3",
    "jest-mock-axios": "4.6.2",
    "jest-mock-extended": "3.0.1",
